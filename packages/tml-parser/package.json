  },
  "devDependencies": {
    "@types/node": "22.15.3",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "eslint": "9.26.0",
    "typescript": "5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": "22.x"
  }
}
